            <ScriptLine>"      Set fso = CreateObject("Scripting.FileSystemObject")"</ScriptLine>
            <ScriptLine>"      Set f = fso.OpenTextFile("C:\Powerbreak III\System\ulc39difw.x3g", ForWriting, True)"</ScriptLine>
