  <Events>
    <OnLoad>
      <Actions>
        <Action>
          <Type>12</Type>
          <IgnoreFailure>false</IgnoreFailure>
          <ScriptLines>
            <ScriptLine>"Dim DB_Type, DB_ServerName, DB_DatabaseName, DB_UserName, DB_Password, FileContents"</ScriptLine>
            <ScriptLine>""</ScriptLine>
            <ScriptLine>"'NW - Reads the configuration file (ulc39difw.x3g)to get the current connection string."</ScriptLine>
            <ScriptLine>"'This file contains the current configuration for the database location."</ScriptLine>
            <ScriptLine>""</ScriptLine>
            <ScriptLine>"Const ForReading = 1"</ScriptLine>
            <ScriptLine>"Set fso = CreateObject("Scripting.FileSystemObject")"</ScriptLine>
            <ScriptLine>"Set MyFile = fso.OpenTextFile("C:\Powerbreak III\System\ulc39difw.x3g", ForReading)"</ScriptLine>
            <ScriptLine>"FileContents = MyFile.ReadAll"</ScriptLine>
            <ScriptLine>"FileContents = split(FileContents, VBCRLF)"</ScriptLine>
            <ScriptLine>"Set MyFile = nothing"</ScriptLine>
            <ScriptLine>"Set fso = nothing"</ScriptLine>
            <ScriptLine>""</ScriptLine>
            <ScriptLine>"Dim conn, rs, SQL"</ScriptLine>
            <ScriptLine>"Set conn = CreateObject("ADODB.Connection")"</ScriptLine>
            <ScriptLine>"Set rs = CreateObject("ADODB.RecordSet")"</ScriptLine>
            <ScriptLine>""</ScriptLine>
            <ScriptLine>"If (Replace(split(FileContents(0), " = """)(1), """", "") = "SQL") Then"</ScriptLine>
            <ScriptLine>"   SQLServer = Replace(split(FileContents(1), " = """)(1), """", "")"</ScriptLine>
            <ScriptLine>"   DatabaseName = Replace(split(FileContents(2), " = """)(1), """", "")"</ScriptLine>
            <ScriptLine>"   DB_Authentication = "SQL""</ScriptLine>
            <ScriptLine>"   SQLUserName = Replace(split(FileContents(4), " = """)(1), """", "")"</ScriptLine>
            <ScriptLine>"   SQLPassword = Replace(split(FileContents(5), " = """)(1), """", "")"</ScriptLine>
            <ScriptLine>"   DBType = "SQL""</ScriptLine>
            <ScriptLine>"ElseIf (Replace(split(FileContents(0), " = """)(1), """", "") = "AccessACCDB") Then"</ScriptLine>
            <ScriptLine>"   AccessDBLocation = Replace(split(FileContents(1), " = """)(1), """", "")"</ScriptLine>
            <ScriptLine>"   DBType = "Access (ACCDB)""</ScriptLine>
            <ScriptLine>"ElseIf (Replace(split(FileContents(0), " = """)(1), """", "") = "AccessMDB") Then"</ScriptLine>
            <ScriptLine>"   AccessDBLocation = Replace(split(FileContents(1), " = """)(1), """", "")"</ScriptLine>
            <ScriptLine>"   DBType = "Access (MDB)""</ScriptLine>
            <ScriptLine>"ElseIf (Replace(split(FileContents(0), " = """)(1), """", "") = "ODBC") Then"</ScriptLine>
            <ScriptLine>"   ODBCName = Replace(split(FileContents(1), " = """)(1), """", "")"</ScriptLine>
            <ScriptLine>"   DBType = "ODBC""</ScriptLine>
            <ScriptLine>"End If"</ScriptLine>
            <ScriptLine>""</ScriptLine>
            <ScriptLine>"'App Status allows the administrator to signify if the application is offline/online."</ScriptLine>
            <ScriptLine>"'At this point, we don't have anything in the application for online/offline; however,"</ScriptLine>
            <ScriptLine>"'if the customer wanted to move in this direction, we could build in a default"</ScriptLine>
            <ScriptLine>"'check to move all of the read/write information to a local database."</ScriptLine>
            <ScriptLine>"Dim AppStatus"</ScriptLine>
            <ScriptLine>"AppStatus = ReadLineFromFile("C:\Powerbreak III\System\AppStatus.ini")"</ScriptLine>
            <ScriptLine>""</ScriptLine>
            <ScriptLine>"Select Case AppStatus"</ScriptLine>
            <ScriptLine>"   Case "APP ONLINE""</ScriptLine>
            <ScriptLine>"      AppOnline = "TRUE""</ScriptLine>
            <ScriptLine>"      AppOffline = "FALSE""</ScriptLine>
            <ScriptLine>"   Case "APP OFFLINE""</ScriptLine>
            <ScriptLine>"      AppOffline = "TRUE""</ScriptLine>
            <ScriptLine>"      AppOnline = "FALSE""</ScriptLine>
            <ScriptLine>"End Select"</ScriptLine>
            <ScriptLine>""</ScriptLine>
            <ScriptLine>""</ScriptLine>
            <ScriptLine>""</ScriptLine>
            <ScriptLine>""</ScriptLine>
            <ScriptLine>""</ScriptLine>
            <ScriptLine>""</ScriptLine>
            <ScriptLine>""</ScriptLine>
            <ScriptLine>""</ScriptLine>
            <ScriptLine>""</ScriptLine>
            <ScriptLine>""</ScriptLine>
            <ScriptLine>""</ScriptLine>
            <ScriptLine>""</ScriptLine>
          </ScriptLines>
        </Action>
      </Actions>
