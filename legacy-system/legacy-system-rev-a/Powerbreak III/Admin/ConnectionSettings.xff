            <ScriptLine>""</ScriptLine>
            <ScriptLine>"AccessDataMDB = "DB_Type = ""AccessMDB""" &amp; VBCRLF &amp; _"</ScriptLine>
            <ScriptLine>""DB_Location = """ &amp; AccessDBLocation &amp; """""</ScriptLine>
            <ScriptLine>""</ScriptLine>
            <ScriptLine>"AccessDataACCDB = "DB_Type = ""AccessACCDB""" &amp; VBCRLF &amp; _"</ScriptLine>
            <ScriptLine>""DB_Location = """ &amp; AccessDBLocation &amp; """""</ScriptLine>
            <ScriptLine>""</ScriptLine>
            <ScriptLine>"ODBCInfo = "DB_Type = ""ODBC""" &amp; VBCRLF &amp; _"</ScriptLine>
            <ScriptLine>""DB_Location = """ &amp; ODBCName &amp; """""</ScriptLine>
            <ScriptLine>""</ScriptLine>
            <ScriptLine>"Const ForReading = 1, ForWriting = 2"</ScriptLine>
            <ScriptLine>"Dim fso, f"</ScriptLine>
            <ScriptLine>""</ScriptLine>
            <ScriptLine>""</ScriptLine>
            <ScriptLine>"If DBType = "SQL" Then"</ScriptLine>
            <ScriptLine>"   f.Write SQLData"</ScriptLine>
            <ScriptLine>"ElseIf DBType = "Access (MDB)" Then"</ScriptLine>
            <ScriptLine>""</ScriptLine>
            <ScriptLine>"   AccessMDBCheck = Split(AccessDBLocation, ".", -1,1)"</ScriptLine>
            <ScriptLine>""</ScriptLine>
            <ScriptLine>"   If AccessMDBCheck(1) = "mdb" OR AccessMDBCheck(1) = "MDB" Then"</ScriptLine>
            <ScriptLine>"      MsgBox "Database Path Saved Successfully""</ScriptLine>
            <ScriptLine>"      Set fso = CreateObject("Scripting.FileSystemObject")"</ScriptLine>
            <ScriptLine>"      Set f = fso.OpenTextFile("C:\Powerbreak III\System\ulc39difw.x3g", ForWriting, True)"</ScriptLine>
            <ScriptLine>"      f.write AccessDataMDB"</ScriptLine>
            <ScriptLine>"   Else"</ScriptLine>
            <ScriptLine>"      MsgBox "ERROR: The database you have selected does not match the database" &amp; VBCRLF &amp; _"</ScriptLine>
            <ScriptLine>"             "option you have selected.""</ScriptLine>
            <ScriptLine>"   End If"</ScriptLine>
            <ScriptLine>""</ScriptLine>
            <ScriptLine>"ElseIf DBType = "Access (ACCDB)" Then"</ScriptLine>
            <ScriptLine>""</ScriptLine>
            <ScriptLine>"   AccessACCDBCheck = Split(AccessDBLocation, ".", -1,1)"</ScriptLine>
